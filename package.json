{
  "name": "storyset",
  "description": "A set of useful tools for Storybook.",
  "version": "0.0.0-development",
  "author": "Richard King <richrdkng@gmail.com> (https://www.richrdkng.com/)",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storyset/storyset.git"
  },
  "bugs": {
    "url": "https://github.com/storyset/storyset/issues"
  },
  "homepage": "https://github.com/storyset/storyset#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "story",
    "stories",
    "storyset",
    "util",
    "utils",
    "tool",
    "tools",
    "storybook"
  ],
  "scripts": {
    "start": "start-storybook -p 6006 --no-open --no-version-updates",
    "lint": "npm run _lint:1 && npm run _lint:2",
    "_lint:1": "tsc --noEmit",
    "_lint:2": "ts-standard | snazzy",
    "test": "jest",
    "prep": "npm run _prep:1 && npm run _prep:2",
    "_prep:1": "rm -rf dist && mkdir dist",
    "_prep:2": "cp LICENSE* README* package.json dist",
    "build": "npm run _build:src && npm run _build:types",
    "_build:src": "node scripts/build.js",
    "_build:types": "tsc -p tsconfig.types.json && tsc-alias -p tsconfig.types.json",
    "build:storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@tsconfig/create-react-app": "^1.0.2",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "@types/react": "^17.0.62",
    "babel-loader": "^8.2.5",
    "esbuild": "^0.18.8",
    "esbuild-node-externals": "^1.8.0",
    "jest": "^28.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^19.0.5",
    "snazzy": "^9.0.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "ts-standard": "^12.0.2",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.6.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@reactory/class-name": "^2.2.0"
  }
}
